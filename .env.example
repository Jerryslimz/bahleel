# Bahleel Configuration

# Auto save scraped data to database
BAHLEEL_AUTO_SAVE=true

# Filter duplicate items based on hash
BAHLEEL_DUPLICATE_FILTER=true

# Default spider settings
BAHLEEL_CONCURRENCY=2
BAHLEEL_REQUEST_DELAY=1

# Proxy settings
PROXY_ENABLED=false
PROXY_URL=
PROXY_HOSTS=*

# JavaScript execution (requires Puppeteer installed)
JS_ENABLED=false
CHROME_PATH=
NODE_PATH=
